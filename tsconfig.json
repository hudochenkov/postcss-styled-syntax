{
	"compilerOptions": {
		"strict": true,
    	"forceConsistentCasingInFileNames": true,
		"target": "ES2020",
		"lib": ["es2020"],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"module": "commonjs",
		"noEmit": true,
		"allowJs": true,
		"skipLibCheck": true,
		"checkJs": true
	},
	"exclude": ["./**/node_modules/**/*"],
	"include": ["./lib/**/*.js"]
}
